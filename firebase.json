{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ], 
     "headers": [ {
      "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
      "headers": [ {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      } ]
    } ]
    , "redirects": [ {
      "source": "/election-dev/index.php/User/sheetLogin",
      "destination": "http://one11tech.com/election-dev/index.php/User/sheetLogin",
      "type": 301
    }]
  
  }
}
